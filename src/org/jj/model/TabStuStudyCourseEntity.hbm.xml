<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="org.jj.model.TabStuStudyCourse" table="tab_stu_study_course" schema="education">
        <id name="id" column="id"/>
        <property name="studentUserId" column="student_user_id"/>
        <property name="courseId" column="course_id"/>
        <property name="note" column="note"/>
        <property name="publishTime" column="publish_time"/>
        <property name="teachUserId" column="teach_user_id"/>
    </class>
</hibernate-mapping>